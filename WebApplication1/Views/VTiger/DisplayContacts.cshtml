@model List<WebApplication1.Models.VTigerContact>

<h2>Contacts</h2>

@if (Model != null && Model.Count > 0)
{
    <table class="table">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Assigned User ID</th>
                <!-- Add more columns as needed -->
            </tr>
        </thead>
        <tbody>
            @foreach (var contact in Model)
            {
                <tr>
                    <td>@contact.firstname</td>
                    <td>@contact.lastname</td>
                    <td>@contact.AssignedUserId</td>
                    <!-- Add more columns as needed -->
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No contacts available.</p>
}
